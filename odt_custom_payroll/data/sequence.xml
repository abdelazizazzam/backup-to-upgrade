<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="allowance_transaction_sequence" model="ir.sequence">
            <field name="name">Allowances Transaction</field>
            <field name="code">hr.payslip.allowance.transaction</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="settlement_sequence" model="ir.sequence">
            <field name="name">Settlement Sequence</field>
            <field name="code">hr.settlement</field>
            <field name="prefix">SET-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>